Rahakott = Math.random()*50  //$ randomiseerib, kui palju raha kliendil onn
tooteHind = 25               // kui palju toode maksab
if (tooteHind > Rahakott){   //v천rdleb kui palju rahakottis raha on tootehinnaga
    console.log("Sa ei saa seda osta seda, sul on puudu")      //vastus kui rahakottis on v채hem raha kui tootehind
}
else console.log("sul on piisavalt, raha j채i 체le")
console.log(Rahakott-tooteHind)
